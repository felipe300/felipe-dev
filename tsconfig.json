{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@styles/*": [
        "src/styles/*"
      ],
      "@data/*": [
        "src/data/*"
      ],
      "@types": [
        "src/types"
      ],
      "@utils/*": [
        "src/utils/*"
      ]
    }
    // "types": [
    //   "@astrojs/image/client"
    // ]
  }
}